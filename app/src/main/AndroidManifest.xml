        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.GroupActivity" />
        <activity android:name=".Activities.PackageActivity" />
        <activity android:name=".Activities.ActorActivity" />
        <activity android:name=".Activities.ObjecActivity" />
        <activity android:name=".Activities.ReqNFunActivity" />
        <activity android:name=".Activities.ReqInfoActivity" />
        <activity android:name=".Activities.ReqFunActivity" />
        <activity android:name=".Activities.ListActivity"></activity>
    </application>

</manifest>